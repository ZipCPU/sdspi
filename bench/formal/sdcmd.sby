[tasks]
prf
prfds prf opt_ds
cvr

[options]
prf: mode prove
cvr: mode cover
prf: depth 8
cvr: depth 200

[engines]
smtbmc

[script]
read -formal sdcmd.v
--pycode-begin--
cmd = "hierarchy -top sdcmd"
cmd += " -chparam OPT_DS %d"  % ( 1 if "op_ds" in tags else 0)
output(cmd)
--pycode-end--
proc -norom
prep -top sdcmd

[files]
../../rtl/sdcmd.v
