[tasks]
prf
prfs prf serdes
cvr
cvrs cvr serdes

[options]
prf: mode prove
prf: depth 5
cvr: mode cover
cvr: depth 96

[engines]
smtbmc

[script]
read -formal sdtxframe.v
--pycode-begin--
cmd = "hierarchy -top sdtxframe"
cmd += " -chparam OPT_SERDES %d" % (1 if "serdes" in tags else 0)
output(cmd)
--pycode-end--
proc -norom
prep -top sdtxframe

[files]
../../rtl/sdtxframe.v
